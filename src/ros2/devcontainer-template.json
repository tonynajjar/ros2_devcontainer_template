{
    "id": "ros2",
    "version": "0.0.1",
    "name": "ros2 devcontainer template",
    "description": "A ros2 devcontainer template",
    "documentationURL": "https://github.com/tonynajjar/ros2_devcontainer_template/tree/main/src/ros2",
    "licenseURL": "https://github.com/tonynajjar/ros2_devcontainer_template/blob/main/LICENSE",
    "keywords": ["ros2"],
    "platforms": ["Any"],
    "options": {
        "rosImage": {
            "type": "string",
            "description": "Which ROS2 distro to use. See https://hub.docker.com/r/osrf/ros/tags for all available tags.",
            "proposals": [
                "osrf/ros:rolling-desktop-full",
                "osrf/ros:jazzy-desktop-full",
                "osrf/ros:iron-desktop-full",
                "osrf/ros:humble-desktop-full"
              ],
            "default": "osrf/ros:jazzy-desktop-full"
        }
    }
}